

## enable fish-like autocomplete by zsh-snap. start.
	znap source zsh-users/zsh-autosuggestions
	znap source zsh-users/zsh-completions
	## choose zshell.dev instead of zsh-users
	# znap source zsh-users/zsh-syntax-highlighting
	znap source z-shell/F-Sy-H

	
	source ~/Git/zsh-autosuggestions/zsh-autosuggestions.plugin.zsh 
	source ~/Git/zsh-completions/zsh-completions.plugin.zsh 
	# source ~/Git/zsh-syntax-highlighting/zsh-syntax-highlighting.plugin.zsh
	source ~/Git/F-Sy-H/F-Sy-H.plugin.zsh


	## autocomplete should be last
	znap source marlonrichert/zsh-autocomplete
	source ~/Git/zsh-autocomplete/zsh-autocomplete.plugin.zsh 	 
## enable fish-like autocomplete by zsh-snap. end.

